CREATE OR REPLACE WAREHOUSE demo_wh_multicluster
WITH
  WAREHOUSE_SIZE = 'X-SMALL'
  MIN_CLUSTER_COUNT = 1
  MAX_CLUSTER_COUNT = 300
  SCALING_POLICY = 'STANDARD'
  AUTO_SUSPEND = 300
  AUTO_RESUME = TRUE
  INITIALLY_SUSPENDED = TRUE
  COMMENT = 'Multi-cluster warehouse for high-concurrency workloads';
